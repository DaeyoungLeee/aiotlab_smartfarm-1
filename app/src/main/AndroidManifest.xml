<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="kr.co.aiotlab.www">
    <!-- MQTT -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!-- 안드로이드 버전9 포그라운드-->
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <!-- 디바이스 번호 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:name="kr.co.aiotlab.www.App"
        android:allowBackup="true"
        android:icon="@drawable/aiotlablogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        android:networkSecurityConfig="@xml/network_security_config"
        tools:ignore="GoogleAppIndexingWarning"
        tools:targetApi="m">
        <activity android:name="kr.co.aiotlab.www.IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="kr.co.aiotlab.www.LoginActivity" />
        <activity
            android:name="kr.co.aiotlab.www.MainActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|orientation|screenSize" />
        <activity android:name="kr.co.aiotlab.www.CalendarActivity" />
        <activity android:name="kr.co.aiotlab.www.BottomSettingActivity" />
        <activity android:name="kr.co.aiotlab.www.SigninActivity" />
        <activity android:name="kr.co.aiotlab.www.SingupActivity" />
        <activity android:name="kr.co.aiotlab.www.SensorControlActivity" />
        <activity android:name="kr.co.aiotlab.www.EnterIPandPort" />
        <activity
            android:name=".Activity_FireAlert"
            android:launchMode="singleTop" />

        <activity
            android:name=".SwitchNameSetting_LED1"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".SwitchNameSetting_LED2"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".SwitchNameSetting_LED3"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".SwitchNameSetting_LED4"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".SwitchNameSetting_Siren1"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".SwitchNameSetting_Siren2"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".SwitchNameSetting_Power1"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".SwitchNameSetting_Power2"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".SwitchNameSetting_Power3"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".SwitchNameSetting_Power4"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".SwitchNameSetting_Power5"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".SwitchNameSetting_Power6"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".SwitchNameSetting_Power7"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity android:name=".IP_Setting_Activity" />


        <activity
            android:name="kr.co.aiotlab.www.SearchCity"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name="kr.co.aiotlab.www.ThresholdTemperature"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name="kr.co.aiotlab.www.ThresholdBrightness"
            android:theme="@style/AppTheme.CustomTheme" />

        <service android:name="kr.co.aiotlab.www.AppService" />
        <service android:name=".AppService2" />
        <service android:name=".AppService_push" />
        <service android:name="org.eclipse.paho.android.service.MqttService" />
        <service
            android:name=".FirebaseInstanceIDService"
            android:stopWithTask="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />

            </intent-filter>
        </service>

    </application>

</manifest>